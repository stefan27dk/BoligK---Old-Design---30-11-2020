<!DOCTYPE html>
<html>
       
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>@ViewData["Title"] - UdledjningsPrototype</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap-reboot.min.css">
    <script src="https://kit.fontawesome.com/0e787d12a9.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="/assets/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Lato:300,400,700">
    <link rel="stylesheet" href="/assets/fonts/ionicons.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/pikaday/1.6.1/css/pikaday.min.css">
    <link rel="stylesheet" href="/assets/css/News-Cards.css">
    <link href="~/css/BoligKøCustomStyle.css" rel="stylesheet" />
    


    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>

</head>




<body>

    <nav class="navbar navbar-dark navbar-expand-lg fixed-top bg-white portfolio-navbar gradient menuBar">
            <div class="container">
                <a class="navbar-brand logo" style="font-size:20px;" href="#">Lejebolig Ansøgnings Portal</a><button data-toggle="collapse" class="navbar-toggler" data-target="#navbarNav"><span class="sr-only">Toggle navigation</span><span class="navbar-toggler-icon"></span></button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="nav navbar-nav ml-auto">
                        <li class="nav-item" role="presentation"><a class="nav-link active" asp-controller="Home" asp-action="Index"><i class="fas fa-home colorGold"></i>Home</a></li>
                        <li class="nav-item" role="presentation"><a class="nav-link" asp-controller="Home" asp-action="Privacy"><i class="far fa-building colorGold"></i>Portfølje</a></li>
                        <li class="nav-item" role="presentation"><a class="nav-link"  asp-action="Ansøg" asp-controller="Ansøger"><i class="fas fa-file-signature colorGold"></i>Ansøg</a></li>
                        <partial style="color: red;" name="_LoginPartial" />
                    </ul>
                </div>
            </div>
        </nav>

       

    <main class="page landing-page">
        @RenderBody()

    </main>



    <footer class="page-footer footerBack">
        <div class="container">
            <div class="links"><a href="#">Om os</a><a href="#">Kontakt</a><a href="#">Rettigheder</a></div>
            <div class="social-icons"><a href="#"><i class="icon ion-social-facebook"></i></a><a href="#"><i class="icon ion-social-instagram-outline"></i></a><a href="#"><i class="icon ion-social-twitter"></i></a></div>
        </div>
    </footer>



    <script src="/assets/js/jquery.min.js"></script>
    <script src="/assets/bootstrap/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pikaday/1.6.1/pikaday.min.js"></script>
    <script src="/assets/js/theme.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>

</html>